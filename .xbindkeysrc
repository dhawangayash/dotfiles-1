"import -window root $HOME/foto/shot/$(date +%Y_%m_%d-%H%M).png"
Mod4+Shift+Print

"alock -bg image:file=$HOME/foto/wall/beheading.jpg -cursor glyph -auth pam >&/dev/null"
Mod4+Shift+F11

"banishmouse"
Mod4+Escape

"moodlight -u"
XF86MonBrightnessUp

"moodlight -d"
XF86MonBrightnessDown

"moodlight -m"
Control+XF86MonBrightnessUp

"mossrat --toggle"
Mod4+Home

"mossrat --stop"
Mod4+End

"mossrat --prev"
Mod4+Prior

"mossrat --next"
Mod4+Next

"mossrat --mute"
XF86AudioMute

"mossrat -d 1"
XF86AudioLowerVolume

"mossrat -i 1"
XF86AudioRaiseVolume

"songrem --add"
Mod4+Shift+Home

"songrem --play"
Mod4+Shift+Next

"songrem --remove"
Mod4+Shift+Delete
