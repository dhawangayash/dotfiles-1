!
! personal preferences

! swap Alt_L with Super_L (my StumpWM Modifier)
remove mod1 = Alt_L
remove mod4 = Super_L
keycode 133 = Alt_L
keycode 64 = Super_L
add mod1 = Alt_L
add mod4 = Super_L

! remove [useless] Caps_Lock..
! make it an easier Escape key instead
!remove lock = Caps_Lock
!keysym Caps_Lock = Escape
