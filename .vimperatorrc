colorscheme milomouse
set shell=zsh
set editor="urxvt -e vim"
set showtabline=0
set insertmode=0
set focuscontent=on
set defsearch=google
set followhints=0
set hintinputs="label,name,value"
set hintmatching=wordstartswith
let clock_format='(%a:%d/%y %t)'
noremap j 3j
noremap k 3k
noremap <C-k> :pageinfo<CR>
noremap <C-S-d> :dialog downloads<CR>
noremap <C-S-a> :dialog addons<CR>
