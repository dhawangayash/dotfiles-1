!!!!!!!!!!!!!!!
! ~/.larswmrc !
! milomouse   !
!!!!!!!!!!!!!!!
!
! Version: larswm 7.5.3 [15-JUL-2004] by Lars Bernhardsson <lab@fnurt.net>
!
! Status bar and window border colors:
!
larswm.foreground: #bd89de
larswm.background: #101010
!
! Status bar font:
!
larswm.font: -misc-fixedzero-medium-r-semicondensed-*-12-110-75-75-c-60-iso10646-1
!
! Number of virtual desktops per physical screen: (1 - 16)
!
larswm.desktops: 7
!
! Virtual desktop names:
!
larswm.?.0.dtname: [H------]
larswm.?.1.dtname: [-A-----]
larswm.?.2.dtname: [--L----]
larswm.?.3.dtname: [---C---]
larswm.?.4.dtname: [----Y--]
larswm.?.5.dtname: [-----O-]
larswm.?.6.dtname: [------N]
!
! Resize windows when tiling? (If not, move to and stack in right track)
!
larswm.?.?.tile_resize: True
! larswm.?.0.tile_resize: False
!
! Width of left track in percent: (5-95)
!
larswm.?.?.left_track_width: 60
larswm.?.2.left_track_width: 80
larswm.?.3.left_track_width: 80
larswm.?.5.left_track_width: 80
!
! Don't retile on focus change?
!
larswm.?.?.skip_focus: True
!
! When clicking to focus, also pass click on to client?
!
larswm.?.?.clickthru: False
!
! Application commands for shortcuts:
! Add one line for each application. Must have different numbers between 0 and 63.
!
! larswm.application.0: command
!
larswm.application.0: urxvt
larswm.application_key.0: Return
larswm.application_mod.0: Mod4+Control
larswm.application.1: urxvt -e tmux -S /tmp/.1009/tmux/xorg
larswm.application_key.1: Return
larswm.application_mod.1: Mod4
larswm.application.2: dmenu_run -b -fn "-misc-fixed-medium-r-semicondensed-*-12-110-75-75-c-60-koi8-r" -l 0 -p "Run: " -nb "#101010" -nf "#bd90de" -sb "#bd90de" -sf "#101010"
larswm.application_key.2: semicolon
larswm.application_mod.2: Mod4
larswm.application.3: larsremote restart
larswm.application_key.3: r
larswm.application_mod.3: Mod4+Shift
larswm.application.4: larsremote exit
larswm.application_key.4: q
larswm.application_mod.4: Mod4+Shift
larswm.application.5: mifo -t
larswm.application_key.5: slash
larswm.application_mod.5: Mod4
larswm.application.6: mifo -n
larswm.application_key.6: bracketright
larswm.application_mod.6: Mod4
larswm.application.7: mifo -p
larswm.application_key.7: bracketleft
larswm.application_mod.7: Mod4
larswm.application.8: mifo -n +
larswm.application_key.8: bracketright
larswm.application_mod.8: Mod4+Shift
larswm.application.9: mifo -p dir
larswm.application_key.9: bracketleft
larswm.application_mod.9: Mod4+Shift
larswm.application.10: mifo -c seek 10
larswm.application_key.10: 0
larswm.application_mod.10: Mod4+Shift
larswm.application.11: mifo -c seek -10
larswm.application_key.11: 9
larswm.application_mod.11: Mod4+Shift
larswm.application.12: mifo -s
larswm.application_key.12: slash
larswm.application_mod.12: Mod4+Shift
larswm.application.13: dmenu_man -b -fn "-misc-fixed-medium-r-semicondensed-*-12-110-75-75-c-60-koi8-r" -l 0 -p "Man: " -nb "#101010" -nf "#bd90de" -sb "#bd90de" -sf "#101010"
larswm.application_key.13: semicolon
larswm.application_mod.13: Mod4+Shift
larswm.application.14: import -window root ${XDG_PICTURES_DIR:-${H:-/howl}/foto}/shot/$(date +%Y_%m_%d-%H%M%S).png
larswm.application_key.14: Print
larswm.application_mod.14: Mod4
larswm.application.15: alock -bg image:file=${XDG_PICTURES_DIR:-${H:-/howl}/foto}/wall/beheading.png -cursor glyph -auth pam >&/dev/null
larswm.application_key.15: End
larswm.application_mod.15: Mod4+Control
larswm.application.16: banishmouse
larswm.application_key.16: Escape
larswm.application_mod.16: Mod4
larswm.application.17: urxvt -e mutt -F ${XDG_CONFIG_DIR:-/howl/conf}/mutt/muttrc
larswm.application_key.17: backslash
larswm.application_mod.17: Mod4
larswm.application.18: ossvol -d
larswm.application_key.18: Down
larswm.application_mod.18: Mod4
larswm.application.19: ossvol -i
larswm.application_key.19: Up
larswm.application_mod.19: Mod4
larswm.application.20: ossvol -m
larswm.application_key.20: v
larswm.application_mod.20: Mod4
!
! Application commands for mouse clicks:
!
! larswm.button1.application: command
! larswm.button2.application: command
larswm.button3.application: larsmenu -popup '*  start  *:exit' 'urxvt:urxvt' 'tmux:urxvt -e tmux -S /tmp/.1009/tmux/xorg' 'luakit:luakit' 'firefox:firefox' '*  exit menu  *:exit'
!
! * In all the class lists, the Instance is optional *
!
! Tool applications: (Will NOT be tiled, will be tiled around and will be sticky)
! Add one line for each class. Must have different numbers between 0 and 63.
!
! larswm.toolclass.0: Class~Instance
!
! Floating applications: (Will always stay on top of other applications)
! Add one line for each class. Must have different numbers between 0 and 63.
!
! larswm.floatclass.0: Class~Instance
!
larswm.floatclass.0: Firefox-bin~firefox-bin
!
! Sticky applications: (Will be visible on all virtual desktops)
! Add one line for each class. Must have different numbers between 0 and 63.
!
! larswm.stickyclass.0: Class~Instance
!
! Applications to include in tiling: (Will be put in the tiled subdesktop)
! Add one line for each class. Must have different numbers between 0 and 63.
!
! larswm.dotileclass.0: Class~Instance
!
larswm.dotileclass.0: URxvt~urxvt
larswm.dotileclass.1: luakit~luakit
larswm.dotileclass.2: Minefield~Navigator
larswm.dotileclass.3: sxiv~sxiv
!
! Associate a window class with a desktop number:
! Must provide both class and desktop number.
!
! larswm.dtclass.0: Class~Instance
! larswm.dtnum.0: 0
!
larswm.dtclass.0: luakit~luakit
larswm.dtnum.0: 3
larswm.dtclass.1: Minefield~Navigator
larswm.dtnum.1: 3
larswm.dtclass.2: Firefox-bin~firefox-bin
larswm.dtnum.2: 3
larswm.dtclass.3: libreoffice-startcenter
larswm.dtnum.3: 2
larswm.dtclass.4: libreoffice-writer
larswm.dtnum.4: 2
larswm.dtclass.5: libreoffice-math
larswm.dtnum.5: 2
larswm.dtclass.6: libreoffice-impress
larswm.dtnum.6: 2
larswm.dtclass.7: libreoffice-calc
larswm.dtnum.7: 2
larswm.dtclass.8: libreoffice-draw
larswm.dtnum.8: 2
larswm.dtclass.9: Gimp~gimp
larswm.dtnum.9: 4
larswm.dtclass.10: warmux~warmux
larswm.dtnum.10: 5
larswm.dtclass.11: Pysol
larswm.dtnum.11: 5
larswm.dtclass.12: Eboard~eboard
larswm.dtnum.12: 5
larswm.dtclass.13: xskat~xskat
larswm.dtnum.13: 5
larswm.dtclass.14: Sudoku-savant~sudoku-savant
larswm.dtnum.14: 5
larswm.dtclass.15: Ossxmix
larswm.dtnum.15: 6
!
! Key bindings: (Modifiers: Shift, Lock, Control, Mod1 or Alt, Mod2, Mod3, Mod4 and Mod5)
!
larswm.prev_screen_key: comma
larswm.prev_screen_mod: Mod4+Control
larswm.next_screen_key: period
larswm.next_screen_mod: Mod4+Control
!
larswm.prev_desktop_key: comma
larswm.prev_desktop_mod: Mod4
larswm.next_desktop_key: period
larswm.next_desktop_mod: Mod4
larswm.prev_desktop_drag_key: comma
larswm.prev_desktop_drag_mod: Mod4+Shift
larswm.next_desktop_drag_key: period
larswm.next_desktop_drag_mod: Mod4+Shift
!
larswm.prev_window_key: space
larswm.prev_window_mod: Mod4+Shift
larswm.next_window_key: space
larswm.next_window_mod: Mod4
!
larswm.raise_notile_key: n
larswm.raise_notile_mod: Mod4+Control
!
larswm.move_northwest_key: w
larswm.move_northwest_mod: Mod4+Control
larswm.move_north_key: k
larswm.move_north_mod: Mod4+Control
larswm.move_northeast_key: e
larswm.move_northeast_mod: Mod4+Control
larswm.move_west_key: h
larswm.move_west_mod: Mod4+Control
larswm.move_center_key: s
larswm.move_center_mod: Mod4+Control
larswm.move_east_key: l
larswm.move_east_mod: Mod4+Control
larswm.move_southwest_key: a
larswm.move_southwest_mod: Mod4+Control
larswm.move_south_key: j
larswm.move_south_mod: Mod4+Control
larswm.move_southeast_key: d
larswm.move_southeast_mod: Mod4+Control
!
larswm.toggle_inc_key: equal
larswm.toggle_inc_mod: Mod4+Control
!
!larswm.move_up_key: k
!larswm.move_up_mod: Mod4+Shift
!larswm.move_down_key: j
!larswm.move_down_mod: Mod4+Shift
!larswm.move_left_key: h
!larswm.move_left_mod: Mod4+Shift
!larswm.move_right_key: l
!larswm.move_right_mod: Mod4+Shift
!larswm.move_select_key: 8
!larswm.move_select_mod: Mod4+Shift
!
larswm.move_aside_key: period
larswm.move_aside_mod: Mod4+Control
larswm.move_aside_other_key: comma
larswm.move_aside_other_mod: Mod4+Control
larswm.move_sd_key: f
larswm.move_sd_mod: Mod4
!
larswm.grow_vert_key: o
larswm.grow_vert_mod: Mod4
larswm.shrink_vert_key: o
larswm.shrink_vert_mod: Mod4+Shift
larswm.grow_horiz_key: 0
larswm.grow_horiz_mod: Mod4
larswm.shrink_horiz_key: 9
larswm.shrink_horiz_mod: Mod4
larswm.grow_both_key: b
larswm.grow_both_mod: Mod4
larswm.shrink_both_key: b
larswm.shrink_both_mod: Mod4+Shift
!
larswm.zoom_vert_key: l
larswm.zoom_vert_mod: Mod4
larswm.zoom_horiz_key: h
larswm.zoom_horiz_mod: Mod4
larswm.zoom_full_key: m
larswm.zoom_full_mod: Mod4
larswm.zoom_full2_key: m
larswm.zoom_full2_mod: Mod4+Shift
!
larswm.hide_key: z
larswm.hide_mod: Mod4
larswm.unhide_key: u
larswm.unhide_mod: Mod4
!
larswm.close_key: BackSpace
larswm.close_mod: Mod4+Control
!
! Desktop shortcuts: Defaults are provided for the first 12 desktops, F1-F12.
! If you have more than 12 desktops, you will have to add key bindings for them
! or just use prev/next desktop to get to where you want.
!
larswm.goto_desktop_key.0: grave
larswm.goto_desktop_mod.0: Mod4
larswm.goto_desktop_key.1: 1
larswm.goto_desktop_mod.1: Mod4
larswm.goto_desktop_key.2: 2
larswm.goto_desktop_mod.2: Mod4
larswm.goto_desktop_key.3: 3
larswm.goto_desktop_mod.3: Mod4
larswm.goto_desktop_key.4: 4
larswm.goto_desktop_mod.4: Mod4
larswm.goto_desktop_key.5: 5
larswm.goto_desktop_mod.5: Mod4
larswm.goto_desktop_key.6: 6
larswm.goto_desktop_mod.6: Mod4
!
!
! End of larswm configuration.
