text/html; /usr/bin/w3m -I %{charset} -T text/html %s; copiousoutput;
image/*; /usr/bin/sxiv %s; test=test -n "$DISPLAY";
video/*; /usr/bin/mplayer2 -msglevel all=0 %s 2>/dev/null; test=test -n "$DISPLAY";
application/pdf; /usr/bin/zathura %s; test=test -n "$DISPLAY";
application/gpg; /usr/bin/gpg --decrypt %s; copiousoutput;
