%% $XDG_CONFIG_DIR/.mostrc
%% milomouse <vincent[at]fea.st>

%% colors:
% candymouse theme
color normal lightgray default
color status black white
color underline blue default
color overstrike magenta default

%% keybindings:
% navigation
setkey up "k"
setkey down "j"
setkey page_up "K"
setkey page_up "^?"
setkey page_down "J"
setkey page_down " "
setkey page_left ","
setkey page_right "."
setkey column_left "h"
setkey column_right "l"
setkey bob "gg"
setkey eob "G"
setkey set_mark "#"
setkey goto_mark "@"
setkey goto_percent "%"
setkey goto_line ":"
setkey search_forward "/"
setkey search_backward "?"
% functionality
setkey find_file "F"
setkey next_file "ff"
setkey tail_mode "$"
setkey toggle_options ";"
setkey toggle_case "C"
setkey toggle_width "W"
setkey extended_key "x"
setkey two_windows "\\"
setkey one_window "|"
setkey other_window "ww"
setkey delete_window "-"
setkey edit "e"
setkey redraw "r"
setkey show_time "t"
setkey show_time "\033[A"
setkey show_time "\033[B"
setkey show_time "\033[D"
setkey show_time "\033[C"
setkey suspend "!"
setkey exit "q"
setkey help "H"
% unbind
unsetkey "b"
unsetkey "B"
unsetkey "c"
unsetkey "d"
unsetkey "D"
unsetkey "f"
unsetkey "M"
unsetkey "X"
unsetkey "T"
unsetkey "u"
unsetkey "U"
unsetkey "v"
unsetkey "V"
unsetkey "z"
unsetkey "Z"
unsetkey "^"
unsetkey "^D"
unsetkey "^F"
unsetkey "^H"
unsetkey "^L"
unsetkey "^M"
unsetkey "^N"
unsetkey "^P"
unsetkey "^KE"
unsetkey "^KG"
unsetkey "^K^B"
unsetkey "^K^J"
unsetkey "^K^M"
unsetkey "^@"
unsetkey "\033$"
unsetkey "\033<"
unsetkey "\033>"
unsetkey "\033OA"
unsetkey "\033OB"
unsetkey "\033OC"
unsetkey "\033OD"
unsetkey "\033OPE"
unsetkey "\033OPO"
unsetkey "\033OPQ"
unsetkey "\033OPS"
unsetkey "\033OPV"
unsetkey "\033OPX"
unsetkey "\033OP\033OR"
unsetkey "\033OP\033On"
unsetkey "\033OP\033Ot"
unsetkey "\033OP\033Ou"
unsetkey "\033OP\033[A"
unsetkey "\033OP\033[B"
unsetkey "\033OP\033[C"
unsetkey "\033OP\033[D"
unsetkey "\033OQ"
unsetkey "\033OR"
unsetkey "\033On"
unsetkey "\033Op"
unsetkey "\033Ot"
